<h2><%= @question.title %> </h2>
<p><%= @question.answer %> </p>
<hr>
<%= link_to "Edit", edit_question_path(@question), :class => 'btn btn-primary' %>
<%= link_to "Delete", question_path(@question), method: :delete, data: {confirm: 'Are you sure you want do delete this?'},
            :class => 'btn btn-danger' %>