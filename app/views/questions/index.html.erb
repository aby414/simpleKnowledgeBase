<h1>FAQ</h1>
<hr>
<%= link_to "Create question", new_question_path(@question), :class => 'btn btn-primary' %>
<% @questions.each do |question| %>
  <div class="card card-body bg-light">
    <h4><%= question.title%></h4>
    <p><%= question.answer %></p>
    <%= link_to "Edit", question_path(question), :class => 'btn btn-default'%>
  </div>
<% end %>

